{
  "name": "merqbiz-web-ui",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/merqbiz/merqbiz-web-ui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/merqbiz/merqbiz-web-ui/issues"
  },
  "homepage": "https://github.com/merqbiz/merqbiz-web-ui#readme",
  "dependencies": {
    "gatsby": "^1.9.250",
    "gatsby-cli": "^1.1.52",
    "gatsby-link": "^1.6.17",
    "gatsby-plugin-react-helmet": "^1.0.5",
    "gatsby-source-filesystem": "^1.5.3",
    "gatsby-transformer-remark": "^1.7.15",
    "gatsby-plugin-react-next": "^1.0.8",
    "gatsby-plugin-postcss-sass": "^1.0.19",
    "autoprefixer": "^8.2.0"
  },
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "format": "prettier --write 'src/**/*.js'",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "eslint src",
    "test": "jest"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.4.3",
    "@storybook/addon-viewport": "^3.4.0",
    "@storybook/react": "^3.4.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.8.0",
    "jest": "^22.4.3",
    "prettier": "^1.11.1",
    "react-test-renderer": "15.6.2",
    "storybook-react-router": "^1.0.1",
    "webpack": "1.15.0"
  },
  "resolutions": {
    "@storybook/**/webpack": "^3.0.0",
    "@storybook/**/postcss-cssnext": "^3.0.0",
    "@storybook/**/postcss-import": "^11.0.0",
    "@storybook/**/postcss-loader": "^2.0.0"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "es2015",
          "react",
          "stage-0"
        ]
      }
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testRegex": "(\\.(test|spec))\\.(jsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache/"
    ],
    "modulePaths": [
      "src"
    ]
  }
}
